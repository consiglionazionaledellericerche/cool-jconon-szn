<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>cnr-snapshots</id>
            <mirrorOf>cnr-snapshots</mirrorOf>
            <url>http://maven.si.cnr.it/content/groups/public-snapshots</url>
        </mirror>
        <mirror>
            <id>nexus</id>
            <mirrorOf>*</mirrorOf>
            <url>http://maven.si.cnr.it/content/groups/public</url>
        </mirror>
        <mirror>
            <id>opencmis-criteria</id>
            <mirrorOf>opencmis-criteria</mirrorOf>
            <url>http://maven.si.cnr.it/content/repositories/opencmis-criteria</url>
        </mirror>
        <mirror>
            <id>clojars</id>
            <mirrorOf>clojars</mirrorOf>
            <url>http://maven.si.cnr.it/content/repositories/clojars</url>
        </mirror>
        <mirror>
            <id>cnr-bandt</id>
            <mirrorOf>cnr-bandt</mirrorOf>
            <url>http://maven.si.cnr.it/content/repositories/cnr-bandt</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>sonar</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <sonar.host.url>
                    http://sonar.si.cnr.it:9000
                </sonar.host.url>
            </properties>
        </profile>
        <profile>
            <id>cool</id>
            <properties>
                <node.path>node</node.path>
            </properties>
        </profile>
        <profile>
            <id>nexus</id>
            <repositories>
                <repository>
                    <id>public</id>
                    <name>public</name>
                    <url>http://maven.si.cnr.it/content/groups/public</url>
                </repository>
                <repository>
                    <id>opencmis-criteria</id>
                    <name>opencmis-criteria</name>
                    <url>http://maven.si.cnr.it/content/repositories/opencmis-criteria</url>
                </repository>
                <repository>
                    <id>clojars</id>
                    <name>clojars</name>
                    <url>http://maven.si.cnr.it/content/repositories/clojars</url>
                </repository>
                <repository>
                    <id>cnr-bandt</id>
                    <name>cnr-bandt</name>
                    <url>http://maven.si.cnr.it/content/repositories/cnr-bandt</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>nexus</activeProfile>
    </activeProfiles>
</settings>